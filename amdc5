class Animal:
    def __init__(self, sound):
        self.sound = sound

    def set_sound(self, new_sound):
        if new_sound != "":
            self.new_sound = new_sound      
        else:
            print("ไม่สามารถตั้งเสียงว่างได้")

    def get_sound(self):
        return self.sound
    
Animal = Animal("ร้อง")
Animal.set_sound("")
Animal.set_sound("เหมี๊ยว")
print(Animal.get_sound())
